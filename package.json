{
  "name": "@dashevo/dapi",
  "version": "1.0.0",
  "description": "Dash Decentralized API (DAPI)",
  "scripts": {
    "start": "node index.js",
    "test": "nyc --check-coverage --lines=34 --branches=12 --functions=34 mocha --reporter=min --recursive test/",
    "test:full-reporting": "nyc --check-coverage --reporter=html --reporter=text --lines=70 --branches=50 --functions=70 mocha test/ --recursive",
    "test:integration": "node test/integration/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/test/**",
      "**/coverage/**"
    ],
    "all": true
  },
  "dependencies": {
    "@dashevo/dapi-db": "git+ssh://git@github.com:dashevo/dapi-db.git",
    "@dashevo/dash-schema": "git+ssh://git@github.com:dashevo/dash-schema.git#v3.0.0-alpha.3",
    "@dashevo/dashcore-lib": "git+ssh://git@github.com:dashevo/dashcore-lib.git#v1.0.0-alpha.15",
    "@dashevo/dashcore-p2p": "git+ssh://git@github.com:dashevo/dashcore-p2p.git",
    "@dashevo/dashd-rpc": "git+ssh://git@github.com:dashevo/dashd-rpc.git",
    "@dashevo/quorums": "git+ssh://git@github.com:dashevo/quorums.git",
    "ajv": "^6.4.0",
    "bloom-filter": "^0.2.0",
    "bluebird": "^3.4.6",
    "crypto": "0.0.3",
    "dotenv": "^6.0.0",
    "eventemitter2": "^4.1.0",
    "extend": "^3.0.1",
    "ipfs-api": "^14.0.4",
    "jayson": "^2.0.4",
    "khal": "^2.0.0",
    "lodash": "^4.17.4",
    "node-cache": "^4.1.1",
    "object-hash": "^1.3.0",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "zeromq": "^4.6.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "4.10.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "istanbul": "^0.4.5",
    "memwatch-next": "^0.3.0",
    "mocha": "^4.1.0",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "proxyquire": "^2.1.0",
    "rewire": "^4.0.1",
    "should": "^11.2.1",
    "sinon": "^4.2.0"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashevo/dapi.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dashevo/dapi/issues"
  },
  "homepage": "https://github.com/dashevo/dapi#readme"
}
