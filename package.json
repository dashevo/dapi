{
  "name": "@dashevo/dapi",
  "version": "0.1.3",
  "description": "Dash Decentralized API (DAPI)",
  "scripts": {
    "start": "node index.js",
    "test": "nyc --check-coverage --lines=42 --branches=20 --functions=40 mocha --reporter=min --recursive test/",
    "test:full-reporting": "nyc --check-coverage --reporter=html --reporter=text --lines=70 --branches=50 --functions=70 mocha test/ --recursive",
    "test:integration": "node test/integration/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "oas:generate": "swagger-jsdoc -d doc/swaggerDef.js lib/rpcServer/**/*.js -o doc/swagger.json"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/test/**",
      "**/coverage/**"
    ],
    "all": true
  },
  "dependencies": {
    "@dashevo/dash-schema": "^3.0.0-alpha.3",
    "@dashevo/dashcore-lib": "^0.16.5",
    "@dashevo/dashcore-p2p": "^1.1.6",
    "@dashevo/dashd-rpc": "^1.2.1",
    "@dashevo/quorums": "^1.0.4",
    "ajv": "^6.7.0",
    "bloom-filter": "^0.2.0",
    "bluebird": "^3.5.3",
    "dotenv": "^6.2.0",
    "eventemitter2": "^5.0.1",
    "extend": "^3.0.2",
    "ipfs-http-client": "^29.0.0",
    "jayson": "^2.1.1",
    "lodash": "^4.17.11",
    "node-cache": "^4.2.0",
    "object-hash": "^1.3.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "zeromq": "^5.1.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "proxyquire": "^2.1.0",
    "rewire": "^4.0.1",
    "should": "^13.2.3",
    "sinon": "^7.2.3",
    "swagger-jsdoc": "^3.2.6"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashevo/dapi.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dashevo/dapi/issues"
  },
  "homepage": "https://github.com/dashevo/dapi#readme"
}
